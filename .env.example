# Copy to .env and fill in values
# Backend and Supabase must share the same JWT_SECRET
JWT_SECRET=replace-with-a-long-random-secret-at-least-32-characters

# Supabase keys (from your Supabase project or local stack)
SUPABASE_ANON_KEY=replace-with-supabase-anon-key
SUPABASE_SERVICE_KEY=replace-with-supabase-service-role-key

# Frontend defaults (used by docker-compose)
VITE_API_URL=http://localhost:8000
VITE_SUPABASE_URL=http://localhost:54321
